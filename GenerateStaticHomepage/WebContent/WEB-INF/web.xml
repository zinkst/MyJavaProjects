<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp">
	<display-name>GenerateStaticHomepage</display-name>
	<servlet>
		<servlet-name>GSServlet</servlet-name>
		<servlet-class>urlaubsbilder.generatestatic.GSServlet</servlet-class>
		<init-param>
			<param-name>ResultPage</param-name>
			<param-value>/Result.jsp</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>GSServlet</servlet-name>
		<url-pattern>GSServlet</url-pattern>
	</servlet-mapping>
</web-app>
